<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Database</title>
    <style>
      section {
        display: flex;
        flex-wrap: wrap;
        padding: 0 4px;
      }
      img {
        max-height: 250px;
        min-width: 250px;
        object-fit: cover;
        vertical-align: bottom;

        margin-top: 8px;
        vertical-align: middle;
      }
    </style>
  </head>
  <body>
    <h1>Database</h1>
    <section>
      <% for (const property in findData) { %>

      <div class="column">
        <img src="<%= findData[property].fileInfo %>" alt="" />
        <h2><%= findData[property].fileName %></h2>
      </div>

      <% } %>
    </section>

    <a href="/home">Add another Screenshot</a>
  </body>
</html>
